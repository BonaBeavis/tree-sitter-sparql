BASE <http://www.example.org>
PREFIX ex: <http://www.example.org>
PREFIX ex: <http://www.example.org>

SELECT ?x {
  ?x ex:p :o
}

PREFIX     :    <http://example.org/ns#>
PREFIX foaf:    <http://xmlns.com/foaf/0.1/>

SELECT ?name
WHERE { ?x foaf:name ?name ; :empId ?emp }
ORDER BY DESC(?emp)
SELECT DISTINCT ( - 1 AS ?x ) ( -1 AS ?y )

FROM <www.example.com>
FROM NAMED <www.example.com>

WHERE {
  () :predicate ( :a :b :c ) .
  _:alice foaf:knows _:bob .
  :DateTimeDescription
     rdf:type owl:Class ;
     rdf:type owl:Class ;
     rdf:type owl:Class .
  ?s ?p0 ?o0,<t> ;
     ?p1 prox:test .
  (?foo [:p :o] (?bar)) .
  [ a :o ] ?p ?o .
  _:s :p "test", :tm, ex:p
  { ?s ?p ?o }
  { ?s ?p ?o }
  FILTER REGEX(?s, "^foo", "i")
  FILTER REGEX(?s, ?a, ?s)
  FILTER REGEX(?s, ?a)
  FILTER RAND()
  FILTER func:foo(?s, "^f[]\boo", "i", $s)
  FILTER strends(?s, "^foo")
  FILTER str("^foo")
  FILTER sameTerm("^foo", ?a)
  {} OPTIONAL { ?s ?p ?o }
  optional { ?s ?p ?o }
  { ?s ?p ?o }
  UNION
  { ?s ?p ?o }
} GROUP BY ?x

VALUES ?a {
  <http://example.org/foo>
